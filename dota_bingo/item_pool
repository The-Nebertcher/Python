@bot.command(name='currentcard')
async def current_card(ctx):
    if str(ctx.message.author.id) not in bot.bingo_cards:
        await ctx.send(f"No bingo card exists for {ctx.message.author.mention}.")
        return  # Exit the function, don't proceed further

    card_table = PrettyTable()
    for row in bot.bingo_cards[str(ctx.message.author.id)]:
        card_table.add_row(row)

    await ctx.send(f"Current bingo card for {ctx.message.author.mention}:\n```\n{card_table}\n```")
    await ctx.message.delete()  # Delete the user's command message.

